{
   "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
   "name": "eos",
   "version-string": "2.1.0",
   "port-version": 1,
   "dependencies": [
      "chainbase",
      "eos-vm",
      "fc",
      "rocksdb",
      "boost-hana",
      "boost-assign",
      "boost-bimap",
      "boost-lockfree",
      "boost-signals2",
      "boost-accumulators"
   ],
   "default-features": ["non-chain"],
   "features": {
      "non-chain" : {
         "description": "depedencies required by components other than chain library",
         "dependencies": [
            "abieos",
            "appbase",
            "amqpcpp",
            "yubihsm",
            "libpqxx",
            "boost-convert",
            "boost-process",
            "boost-beast",
            "boost-dll"
         ]
      },
      "vcpkg-llvm": {
         "description": "LLVM from vcpkg",
         "dependencies": [
            {
               "name": "llvm",
               "default-features": false,
               "features": [
                  "enable-rtti"
               ]
            }
         ]
      }
   },
   "builtin-baseline": "6e024e744e7717c06ddacd5089401109c6298553", 
   "overrides": [
      { "name": "libpqxx", "version": "7.2.1" }
  ]
}